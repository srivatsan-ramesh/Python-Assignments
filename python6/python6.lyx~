\end_layout

\begin_layout Plain Layout

plot(t,solveDE( [1,0,2,0], [1,0,3,0,0], t),'r',label='$x(t)$')
\end_layout

\begin_layout Plain Layout

plot(t,solveDE( [2,0], [1,0,3,0,0], t),'b',label='$y(t)$')
\end_layout

